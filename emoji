sys.stdout = codecs.getwriter('utf-8')(sys.stdout)

def usage():
    message = """
    Usage: emoji [Options] [Message ...]
    Options:
       -h --help
          Print this message
       -l --list 
          Print all emoji-characters with the name 
       -a --all 
          Print all emoji-characters without the name 
    """
    print(message)

def showList():
    keys = sorted(emoji_dict.keys())
    for key in keys:
        print key + "\t" + emoji_dict[key]

def showEmojies():
    keys = sorted(emoji_dict.keys())
    for key in keys:
        sys.stdout.write(emoji_dict[key])
    print ""
    
def main():
    try:
        opts, args = getopt.getopt(sys.argv[1:], "ahl", ["all","help", "list"])
    except getopt.GetoptError:
        usage()
        sys.exit(2)

    for o, a in opts:
        if o in ("-a", "--all"):
            showEmojies()
            sys.exit()
        if o in ("-l", "--list"):
            showList()
            sys.exit()
        if o in ("-h", "--help"):
            usage()
            sys.exit()

    for key in args:
        if key in emoji_dict:
            print emoji_dict[key]

main()
